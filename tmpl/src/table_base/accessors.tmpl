        }
        if err = tmpl.genColSetter(table, col, _w); err != nil {return}
        if col.IsNullable{
            if err = tmpl.genColNullSetter(table, col, _w); err != nil {return}
        }

        if col.IsReference() {
            if err = tmpl.genColReferenceGetter(table, col, _w); err != nil {return}
