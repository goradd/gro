            } else {
                if err = tmpl.genColNotNullReferenceSetter(table, col, _w); err != nil {return}
            }
        }
    }

    if err = tmpl.genAliasGetter(table, _w); err != nil {return}
    if err = tmpl.genIsNew(table, _w); err != nil {return}

    return
}

